{
    "containers": {
      "backend": {
        "image": "scuba13/amacoon_service:latest",
        "ports": [
          {
            "8080": "HTTP"
          }
        ],
        "environment": [],
        "command": []
      },
      "nginx": {
        "image": "nginx:latest",
        "ports": [
          {
            "80": "HTTP"
          }
        ],
        "environment": [],
        "command": [],
        "volumes": [
          {
            "hostPath": "./nginx.conf",
            "containerPath": "/etc/nginx/conf.d/default.conf",
            "permission": "rw"
          }
        ]
      }
    },
    "publicEndpoint": {
      "containerName": "nginx",
      "containerPort": 80,
      "healthCheck": {
        "healthyThreshold": 2,
        "unhealthyThreshold": 2,
        "timeoutSeconds": 5,
        "intervalSeconds": 10,
        "path": "/",
        "successCodes": "200-499"
      }
    }
  }
  