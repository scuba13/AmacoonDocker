{
    "version": "3.3",
    "services": {
      "backend": {
        "image": "scuba13/amacoon_service:latest",
        "ports": [
          "8080"
        ],
        "networks": [
          "mynetwork"
        ]
      },
      "nginx": {
        "image": "nginx:latest",
        "ports": [
          "80:80"
        ],
        "volumes": [
          "./nginx.conf:/etc/nginx/conf.d/default.conf"
        ],
        "depends_on": [
          "backend"
        ],
        "networks": [
          "mynetwork"
        ],
        "publicEndpoint": true
      }
    },
    "networks": {
      "mynetwork": {
        "driver": "bridge"
      }
    }
  }
  