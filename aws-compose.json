{
    "containers": {
      "version": "1",
      "services": {
        "backend": {
          "image": "scuba13/amacoon_service:latest",
          "ports": [
            {
              "containerPort": 8080
            }
          ],
          "environment": [],
          "command": []
        },
        "nginx": {
          "image": "nginx:latest",
          "ports": [
            {
              "containerPort": 80
            }
          ],
          "environment": [],
          "command": [],
          "volumes": [
            {
              "hostPath": "./nginx.conf",
              "containerPath": "/etc/nginx/conf.d/default.conf",
              "permission": "rw"
            }
          ],
          "dependsOn": [
            "backend"
          ],
          "publicEndpoint": true
        }
      },
      "networks": {
        "mynetwork": {
          "driver": "bridge"
        }
      }
    },
    "service": "amacoon"
  }
  